{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Users",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "display_name": {
      "type": "string"
    },
    "photo_url": {
      "type": "string",
      "format": "uri"
    },
    "uid": {
      "type": "string"
    },
    "phone_number": {
      "type": "string",
      "pattern": "^\\+?[1-9]\\d{1,14}$"
    },
    "birthday": {
      "type": "string",
      "format": "date"
    },
    "home_town": {
      "type": "string"
    }
  },
  "required": ["email", "uid"]
}
